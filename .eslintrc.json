{
    "plugins": ["prettier"],
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": ["xo","prettier"],
    "overrides": [
        {
            "extends": [
                "xo-typescript"
            ],
            "files": [
                "*.ts",
                "*.tsx"
            ],
            "rules": {
                "@typescript-eslint/indent": ["error", 2]
            }
        }
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error"
    }
}
